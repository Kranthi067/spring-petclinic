pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                echo 'Hello World'git branch: 'feature/2025.03.14', url: 'git@github.com:Kranthi067/spring-petclinic.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
    }
}
stage('test') {
            steps {
                sh 'mvn test'
            }
}
stage('test result report') {
            steps {
                junit 'target/surefire-reports/*.xml'
				}
				}
stage('Artifacts') {
            steps {
               archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
            }
}
stage('deploy') {
            steps {
                echo 'Hello World'
}
}